                                            <span class="d-block overflow-hidden">@Model.OtherEmail</span>
                                        </li>
                                    </ul>
                                </div>
                                <div class="col-12 col-md-6 col-lg-4">
                                    <ul class="no-list-style">
                                        <li class="mb-2 pb-3">
                                            <span class="text-bold-500 primary"><a>@*<i class="icon-present font-small-3"></i>*@ Phone:</a></span>
                                            <span class="d-block overflow-hidden">@Model.Phone</span>
                                        </li>
                                    </ul>
                                </div>
                                <div class="col-12 col-md-6 col-lg-4">
                                    <ul class="no-list-style">
                                        <li class="mb-2 pb-3">
                                            <span class="text-bold-500 primary"><a>@*<i class="icon-present font-small-3"></i>*@ Mobile:</a></span>
                                            <span class="d-block overflow-hidden">@Model.Mobile</span>
                                        </li>
                                    </ul>
                                </div>
                                <div class="col-12 col-md-6 col-lg-4">
                                    <ul class="no-list-style">
                                        <li class="mb-2 pb-3">
                                            <span class="text-bold-500 primary"><a>@*<i class="icon-present font-small-3"></i>*@ Account No:</a></span>
                                            <span class="d-block overflow-hidden">@Model.AccountNo</span>
                                        </li>
                                    </ul>
                                </div>
                                <div class="col-12 col-md-6 col-lg-4">
                                    <ul class="no-list-style">
                                        <li class="mb-2 pb-3">
                                            <span class="text-bold-500 primary"><a>@*<i class="icon-present font-small-3"></i>*@ Status:</a></span>
                                            <span class="d-block overflow-hidden">@Model.StatusText</span>
                                        </li>
                                    </ul>
                                </div>
                                <div class="col-12 col-md-6 col-lg-4">
                                    <ul class="no-list-style">
                                        <li class="mb-2 pb-3">
                                            <span class="text-bold-500 primary"><a>Created By:</a></span>
                                            @if (Model.CreatedByUser != null)
                                            {
                                                <span class="d-block overflow-hidden">@Model.CreatedByUser.FirstName @Model.CreatedByUser.LastName (@Model.CreatedByUser.Role.Name) </span>
                                            }
                                        </li>
                                    </ul>
                                </div>
                                <div class="col-12 col-md-6 col-lg-4">
                                    <ul class="no-list-style">
                                        <li class="mb-2 pb-3">
                                            <span class="text-bold-500 primary"><a>Created On:</a></span>
                                            <span class="d-block overflow-hidden">@Model.CreatedOn.ToString("dd MMM, yyyy")</span>
                                        </li>
                                    </ul>
                                </div>
                                <div class="col-12 col-md-6 col-lg-4">
                                    <ul class="no-list-style">
                                        <li class="mb-2 pb-3">
                                            <span class="text-bold-500 primary"><a>Last Modified By:</a></span>
                                            @if (Model.ModifiedByUser != null)
                                            {
                                                <span class="d-block overflow-hidden">@Model.ModifiedByUser.FirstName @Model.ModifiedByUser.LastName (@Model.ModifiedByUser.Role.Name) </span>
                                            }
                                        </li>
                                    </ul>
                                </div>
                                <div class="col-12 col-md-6 col-lg-4">
                                    <ul class="no-list-style">
                                        <li class="mb-2 pb-3">
                                            <span class="text-bold-500 primary"><a>Last Modified On:</a></span>
                                            <span class="d-block overflow-hidden">@Model.ModifiedOn.ToString("dd MMM, yyyy")</span>
                                        </li>
                                    </ul>
                                </div>
                            </div>

