@model InvVendorDto
@{
    ViewData["Title"] = "Vendor Details";
}

<section id="about">
    <div class="row">
        <div class="col-12">
            <div class="content-header">Vendor Details</div>
            
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12">
            @if (Model.Response.ErrorOccured)
            {
                <div class="row" style="margin:auto !important">
                    <div class="col-md-12">
                        <div class="form-group" style="margin:auto !important">
                            @if (Model.Response.ErrorOccured)
                            {
                                <div class="alert alert-danger alert-dismissible">
                                    <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                                    <strong>Error!</strong> @Model.Response.ErrorMessage
                                </div>
                            }
                        </div>
                    </div>
                </div>
            }
            @if (Model.Id != null && Model.Id != 0)
            {
                <div class="card">
                    <div class="card-header">
                        <h5>Vendor Information</h5>
                        <label asp-for="@Model.VendorCode">Vendor</label>&nbsp;<span class="red"></span>
                        <span type="text" class=" border-grey red">#@Model.VendorCode</span>
                    </div>
                    <div class="card-content" style="padding-top:0px;">
                        <div class="card-body" style="padding-top:0px;">
                            <hr>
                            <div class="row">
                                <div class="col-12 col-md-6 col-lg-4">
                                    <ul class="no-list-style">
                                        <li class="mb-2 pb-3">
                                            <span class="text-bold-500 primary"><a>@*<i class="icon-present font-small-3"></i>*@ Contact Name:</a></span>
                                            <span class="d-block overflow-hidden">@Model.ContactName</span>
                                        </li>
                                    </ul>
                                </div>

                                <div class="col-12 col-md-6 col-lg-4">
                                    <ul class="no-list-style">
                                        <li class="mb-2 pb-3">
                                            <span class="text-bold-500 primary"><a>@*<i class="icon-present font-small-3"></i>*@ Company Name:</a></span>
                                            <span class="d-block overflow-hidden">@Model.CompanyName</span>
                                        </li>
                                    </ul>
                                </div>

                                <div class="col-12 col-md-6 col-lg-4">
                                    <ul class="no-list-style">
                                        <li class="mb-2 pb-3">
                                            <span class="text-bold-500 primary"><a>@*<i class="icon-present font-small-3"></i>*@ City:</a></span>
                                            <span class="d-block overflow-hidden">@Model.City</span>
                                        </li>
                                    </ul>
                                </div>

                                <div class="col-12 col-md-6 col-lg-4">
                                    <ul class="no-list-style">
                                        <li class="mb-2 pb-3">
                                            <span class="text-bold-500 primary"><a>@*<i class="icon-present font-small-3"></i>*@ Country:</a></span>
                                            <span class="d-block overflow-hidden">@Model.Country</span>
                                        </li>
                                    </ul>
                                </div>

                                <div class="col-12 col-md-6 col-lg-4">
                                    <ul class="no-list-style">
                                        <li class="mb-2 pb-3">
                                            <span class="text-bold-500 primary"><a>@*<i class="icon-present font-small-3"></i>*@ Address:</a></span>
                                            <span class="d-block overflow-hidden">@Model.Address</span>
                                        </li>
                                    </ul>
                                </div>

                                <div class="col-12 col-md-6 col-lg-4">
                                    <ul class="no-list-style">
                                        <li class="mb-2 pb-3">
                                            <span class="text-bold-500 primary"><a>@*<i class="icon-present font-small-3"></i>*@ Primary Email:</a></span>
                                            <span class="d-block overflow-hidden">@Model.PrimaryEmail</span>
                                        </li>
                                    </ul>
                                </div>

                                <div class="col-12 col-md-6 col-lg-4">
